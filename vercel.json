{
    "version": 2,
    "build": {
        "env": {
            "CI": "true",
            "SKIP_INSTALL": "true"
        }
    },
    "builds":[
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
        
    ]
}