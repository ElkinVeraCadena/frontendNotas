{
    "version": 2,
    "build": {
        "env": {
            "CI": "true",
            "NPM_FLAGS": "--force"
        },
    "builds":[
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}