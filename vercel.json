{
    "version": 2,
    "builds":[
        {
            "src": "./index.js",
            "use": "@vercel/node"
        },
        {
            "src": "./package.json",
            "use": "@vercel/static-build"
        }
    ],
    "install": {
        "ignoreScripts": true
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
        
    ]
}