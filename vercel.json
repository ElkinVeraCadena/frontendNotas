{
    "version": 2,
    "builds":[
        {
            "src": "./index.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "env": {
        "NPM_FLAGS": "--force"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}